{{#if errors}}
{{#each errors}}
<div class="alert alert-danger" role="alert">
  {{{this.message}}}
</div>
{{/each}}
{{/if}}


{{#if success_msg}}
<div class="alert alert-warning" role="alert">
  {{{success_msg}}}
</div>
{{/if}}

{{#if warning_msg}}
<div class="alert alert-danger" role="alert">
  {{{warning_msg}}}
</div>
{{/if}}